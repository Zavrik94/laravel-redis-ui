		"laravel",
		"redis",
		"ui"
	],
	"type" : "package",
	"require" : {
		"php" : ">=8.0",
		"illuminate/console" : "^9.0",
		"illuminate/database" : "^9.0",
		"illuminate/filesystem" : "^9.0",
		"illuminate/auth": "^9.0",
		"predis/predis" : "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^4.0",
		"mockery/mockery" : "~0.9.4"
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Fei Kwok",
			"email" : "fkwok00@gmail.com"
		},
        {
			"name" : "Artem Zavrazhyn",
			"email" : "arty.zavrik@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-4" : {
			"Zavrik\\RedisUI\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Zavrik\\RedisUI\\LaravelRedisUIServiceProvider"
			]
		}
